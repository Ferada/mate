    <!ENTITY sensors "http://www.imis.uni-luebeck.de/mate/sensors#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.imis.uni-luebeck.de/mate/sensors#"
     xml:base="http://www.imis.uni-luebeck.de/mate/sensors"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:mate="http://www.imis.uni-luebeck.de/mate#">
    <owl:Ontology rdf:about="http://www.imis.uni-luebeck.de/mate/sensors">
        <owl:imports rdf:resource="http://www.imis.uni-luebeck.de/mate"/>
	<rdfs:label xml:lang="en">MATe Sensors Ontology</rdfs:label>
	<rdfs:label xml:lang="de">MATe Sensor Ontologie</rdfs:label>
    </owl:Ontology>

    <owl:ObjectProperty rdf:about="&sensors;jid">
	<rdfs:domain>
	    <owl:Class>
	        <owl:unionOf rdf:parseType="Collection">
		    <owl:Class rdf:about="&sensors;MikeSensorValue"/>
		    <owl:Class rdf:about="&sensors;DesktopSensorValue"/>
		    <owl:Class rdf:about="&sensors;CubeSensorValue"/>
		    <owl:Class rdf:about="&sensors;DoorSensorValue"/>
		</owl:unionOf>
	    </owl:Class>
	</rdfs:domain>
	<rdfs:label xml:lang="en">Jabber ID</rdfs:label>
	<rdfs:label xml:lang="de">Jabber ID</rdfs:label>
    </owl:ObjectProperty>

    <mate:PrimaryKey rdf:ID="JidKey">
        <rdf:first rdf:resource="&sensors;jid"/>
    	<rdf:rest rdf:resource="&rdf;nil"/>
    </mate:PrimaryKey>

    <!-- MIKE SENSOR -->

    <owl:Class rdf:about="&sensors;MikeSensorValue">
    	<mate:primaryKey rdf:resource="&sensors;JidKey"/>
	<rdfs:label xml:lang="en">Mike Sensor Value</rdfs:label>
	<rdfs:label xml:lang="de">Mike-Sensorwert</rdfs:label>
    </owl:Class>

    <owl:ObjectProperty rdf:about="&sensors;mikeSensorSpeaker">
	<rdfs:domain rdf:resource="&sensors;MikeSensorValue"/>
	<!-- <rdfs:range rdf:resource="&xsd;string"/> --> <!-- has this one a specific range? -->
	<rdfs:label xml:lang="en">Mike Sensor Speaker</rdfs:label>
	<rdfs:label xml:lang="en">Mike-Sensorsprecher</rdfs:label>
    </owl:ObjectProperty>

    <!-- DESKTOP SENSOR -->

    <owl:Class rdf:about="&sensors;DesktopSensorValue">
    	<mate:primaryKey rdf:resource="&sensors;JidKey"/>
	<rdfs:label xml:lang="en">Desktop Sensor Value</rdfs:label>
	<rdfs:label xml:lang="de">Desktopsensorwert</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="&sensors;DesktopSensorFrequency">
	<owl:oneOf rdf:parseType="Collection">
	    <owl:Thing rdf:about="#inactive"/>
	    <owl:Thing rdf:about="#active"/>
	    <owl:Thing rdf:about="#very_active"/>
	</owl:oneOf>
	<rdfs:label xml:lang="en">Desktop Sensor Frequency</rdfs:label>
	<rdfs:label xml:lang="de">Desktopsensorfrequenz</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="&sensors;DesktopSensorProgram">
	<owl:oneOf rdf:parseType="Collection">
	    <owl:Thing rdf:about="#unknown"/>
	    <owl:Thing rdf:about="#text"/>
	    <owl:Thing rdf:about="#browser"/>
	</owl:oneOf>
	<rdfs:label xml:lang="en">Desktop Sensor Program</rdfs:label>
	<rdfs:label xml:lang="de">Desktopsensorprogramm</rdfs:label>
    </owl:Class>

    <owl:ObjectProperty rdf:about="&sensors;desktopSensorProgram">
        <rdfs:domain rdf:resource="&sensors;DesktopSensorValue"/>
        <rdfs:range rdf:resource="&sensors;DesktopSensorProgram"/>
	<rdfs:label xml:lang="en">desktop sensor program</rdfs:label>
	<rdfs:label xml:lang="de">Desktopsensorprogramm</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&sensors;desktopSensorFrequency">
        <rdfs:domain rdf:resource="&sensors;DesktopSensorValue"/>
        <rdfs:range rdf:resource="&sensors;DesktopSensorFrequency"/>
	<rdfs:label xml:lang="en">desktop sensor frequency</rdfs:label>
	<rdfs:label xml:lang="de">Desktopsensorfrequenz</rdfs:label>
    </owl:ObjectProperty>
 
    <!-- CUBE SENSOR -->

    <owl:Class rdf:about="&sensors;CubeSensorValue">
    	<mate:primaryKey rdf:resource="&sensors;JidKey"/>
	<rdfs:label xml:lang="en">Cube Sensor Value</rdfs:label>
	<rdfs:label xml:lang="de">Cube-Sensorwert</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="&sensors;CubeSensorState">
	<owl:oneOf rdf:parseType="Collection">
	    <owl:Thing rdf:about="#shortBreak"/>
	    <owl:Thing rdf:about="#longBreak"/>
	    <owl:Thing rdf:about="#reading"/>
	    <owl:Thing rdf:about="#writing"/>
	    <owl:Thing rdf:about="#meeting"/>
	</owl:oneOf>
	<rdfs:label xml:lang="en">Cube Sensor State</rdfs:label>
	<rdfs:label xml:lang="de">Cube-Sensorzustand</rdfs:label>
    </owl:Class>

    <owl:ObjectProperty rdf:about="&sensors;cubeSensorState">
	<rdfs:domain rdf:resource="&sensors;CubeSensorValue"/>
	<rdfs:range rdf:resource="&sensors;CubeSensorState"/>
	<rdfs:label xml:lang="en">cube state</rdfs:label>
	<rdfs:label xml:lang="de">Cube-Sensorzustand</rdfs:label>
    </owl:ObjectProperty>

    <!-- DOOR SENSOR -->

    <owl:Class rdf:about="&sensors;DoorSensorValue">
    	<mate:primaryKey rdf:resource="&sensors;JidKey"/>
	<rdfs:label xml:lang="en">Door Sensor Value</rdfs:label>
	<rdfs:label xml:lang="de">T端rsensorwert</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="&sensors;DoorSensorState">
	<owl:oneOf rdf:parseType="Collection">
	    <owl:Thing rdf:about="#uninterruptible"/>    <!-- 0 -->
	    <owl:Thing rdf:about="#interruptible"/>      <!-- 1 -->
	    <owl:Thing rdf:about="#maybeInterruptible"/> <!-- 2 -->
	</owl:oneOf>
	<rdfs:label xml:lang="en">Door Sensor State</rdfs:label>
	<rdfs:label xml:lang="de">T端rsensorzustand</rdfs:label>
    </owl:Class>


